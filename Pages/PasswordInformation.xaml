<Page x:Class="OlibPasswordManager.Pages.PasswordInformation"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:c="clr-namespace:OlibPasswordManager"
      mc:Ignorable="d" 
      FontFamily="{StaticResource Montserrat}"
      Title="JGftyILgb458"
      d:DesignHeight="850" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <c:PasswordInformationControl x:Name="PageControl"/>
        <Border Grid.Row="1" Background="{DynamicResource GlobalBorderColor}" BorderBrush="{DynamicResource PageBordersColor}" BorderThickness="0,1,1,1">
            <StackPanel Orientation="Horizontal">
                <Button Content="{DynamicResource ChangeP}" Margin="10" Click="ChangedPassword"/>
            </StackPanel>
        </Border>
    </Grid>
</Page>
