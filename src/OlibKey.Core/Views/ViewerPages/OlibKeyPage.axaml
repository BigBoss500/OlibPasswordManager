<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OlibKey.Core.Views.ViewerPages.OlibKeyPage">
    <Panel>
        <Viewbox Margin="100" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Path Data="{StaticResource OlibKeyIconRegular}"
                  Fill="{StaticResource TextFillColorSecondaryBrush}"
                  Stretch="Uniform" />
        </Viewbox>
        
        <Viewbox MaxWidth="150" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="20">
            <Path Data="{StaticResource OlibKeyText}"
                  Fill="{StaticResource TextFillColorSecondaryBrush}"
                  Stretch="Uniform" />
        </Viewbox>
    </Panel>
</UserControl>
